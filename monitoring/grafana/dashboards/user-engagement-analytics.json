          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH intervention_stats AS (\n  SELECT \n    intervention_type,\n    delivery_channel,\n    COUNT(*) as sent_count,\n    COUNT(CASE WHEN status = 'delivered' THEN 1 END) as delivered_count,\n    COUNT(CASE WHEN response_status = 'clicked' OR response_status = 'converted' THEN 1 END) as response_count\n  FROM analytics.intervention_events \n  WHERE created_at > NOW() - INTERVAL '$time_period'\n    AND ($user_segment = 'All' OR user_id IN (\n      SELECT user_id FROM analytics.user_profiles \n      WHERE segment IN ($user_segment)\n    ))\n  GROUP BY intervention_type, delivery_channel\n)\nSELECT \n  intervention_type || ' (' || delivery_channel || ')' as \"Intervention\",\n  CASE \n    WHEN sent_count > 0 THEN (response_count * 100.0 / sent_count)\n    ELSE 0 \n  END as \"Conversion Rate\"\nFROM intervention_stats\nWHERE sent_count > 0\nORDER BY \"Conversion Rate\" DESC\nLIMIT 10",
          "refId": "A",
          "sql": {
